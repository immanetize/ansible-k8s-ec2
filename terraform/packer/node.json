{
  "variables": {
    "region": "us-west-2",
    "crio_version": "1.19",
    "cluster_name": "cluster0",
    "kube_release": "1.19.4-0"
  },
  "provisioners": [
    {
      "type": "ansible",
      "playbook_file": "./ansible/image_prep.yml",
      "extra_arguments": [
        "--extra-vars",
        "cluster_name={{ user `cluster_name` }} crio_version={{ user `crio_version` }} kube_release={{ user `kube_release` }}"
      ]
    }
  ]   
}


